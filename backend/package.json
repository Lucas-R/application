{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "typeorm": "typeorm-ts-node-commonjs",
    "m:run": "npx typeorm migration:run -d ./dist/database/config/connection.js",
    "m:revert": "npx typeorm migration:revert -d ./dist/database/config/connection.js",
    "m:create": "cross-env npx typeorm migration:create src/database/migrations/$npm_config_name",
    "e:create": "cross-env npx typeorm entity:create src/database/entities/$npm_config_name",
    "s:create": "cross-env npx typeorm subscriber:create src/database/subscribers/$npm_config_name"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "pg": "^8.14.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
